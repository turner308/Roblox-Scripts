        tab7:dropdown("Hop Until", true, {
            location = settings.spawnSets;
            list = {
                {
                    Name = "Jin Spawns";
                    flag = "JinSpawn";
                };
                {
                    Name = "Weapon Spawns";
                    flag = "WeaponSpawn";
