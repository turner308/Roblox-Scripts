        old = hookfunction(getrenv().table.insert, function(tab, ...)
            if #tab == 1 and not checkcaller() then
                for i,v in pairs(tab) do
                    if typeof(v) == 'Vector3' or typeof(v) == 'CFrame' then
                        return
                    end
                end
            end
            
            return old(tab, ...)
        end)
