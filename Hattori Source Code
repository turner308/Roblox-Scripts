                            settings.spawnSets.cachedServers[game.JobId] = true
                            savedsettings:save("SL2", settings)
                            hasCalled = true
                            syn.queue_on_teleport(codeWhenHopping)
                            createWarPubs(5824792748)
                        end
                    end
                end
            end)
        end

