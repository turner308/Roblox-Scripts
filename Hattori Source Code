	local function switchKeys()
		if isSwitchEnabled then
			origEquip1.Name = "Equip2"
			origEquip2.Name = "Equip1"
		else
			origEquip1.Name = "Equip1"
			origEquip2.Name = "Equip2"
		end
	end

